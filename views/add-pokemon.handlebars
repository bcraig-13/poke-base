
   <div class="container">
      <h2>Add a pokemon here</h2>
    
      <div class="row">
        <div class="col-md-4"> </div>

        <div class="col-md-4">
          <div class="input-group">
            <div class="form-outline">
              <input type="search" id="searchVal" class="form-control"/>
              {{!-- <label class="form-label" for="form1"></label> --}}
            </div>
            <button id="search" type="button" class="btn btn-primary" style="width: 210px; height: 38px;">
              <i class="fas fa-search">Where's that pokemon?!</i>
            </button>
          </div>
            <div class="card" style="width: 26rem; height: 40rem; align-self: center;">
                <img src="stylesheets/images/screen.jpg" class="card-img-top" alt="empty screen">
                <div class="card-body">
                  <h5 class="card-title">Name: <span id="pkmnName"></span></h5>
                  <p class="card-text">Type: <span id="typeOne"></span><span id="typeTwo">/</span></p>
                  <p class="card-text">Level: <input type="number"></p>
                  <p class="card-text"></p>
                </div>
              </div>
              <button type="button" class="btn btn-primary" style="margin-left: 140px; margin-top: 5px;">
                <i class="fas fa-search">Add to collection</i>
              </button>
              <span id="message"></span>
      </div>
      <div class="col-md-4"> </div>
     </div>
    </div>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>  
